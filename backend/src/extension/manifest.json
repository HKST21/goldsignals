{
  "manifest_version": 3,
  "name": "Telegram Message Monitor",
  "version": "1.0",
  "description": "Monitors Telegram messages for analysis",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://web.telegram.org/*"],
    "js": ["dist/content.js"]
  }],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}